    "aws-xray-sdk-core": "^3.3.3",
    "axios": "^0.21.4",
    "lodash": "^4.17.21",
    "pino": "^6.13.2",
    "serialize-error": "^8.1.0",
    "source-map-support": "^0.5.20"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
