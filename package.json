    "@typescript-eslint/parser": "^4.31.1",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "mockdate": "^3.0.5",
    "prettier": "^2.4.1",
    "randomstring": "^1.2.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
