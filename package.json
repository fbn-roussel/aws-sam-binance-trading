{
  "name": "@hastobegood/crypto-bot",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^14"
  },
  "scripts": {
    "clean": "rm -rf dist .aws-sam reports",
    "build": "yarn tsc",
    "bundle": "yarn webpack",
    "bundle:analyze": "yarn webpack && yarn webpack-bundle-analyzer dist/webpack/stats.json",
    "deploy:test": "sam build && sam deploy --config-env test",
    "deploy:prod": "sam build && sam deploy --config-env prod",
    "test": "jest",
    "lint": "yarn eslint . --ext .ts --format json -o reports/eslint-report.json",
    "lint:fix": "yarn eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.36.1",
    "@aws-sdk/client-secrets-manager": "^3.36.1",
    "@aws-sdk/client-sqs": "^3.36.1",
    "@aws-sdk/lib-dynamodb": "^3.36.1",
    "aws-xray-sdk-core": "^3.3.3",
    "axios": "^0.23.0",
    "lodash": "^4.17.21",
    "pino": "^6.13.3",
    "serialize-error": "^8.1.0",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.84",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.10.6",
    "@types/pino": "^6.3.11",
    "@types/randomstring": "^1.1.7",
    "@types/source-map-support": "^0.5.4",
    "@types/uuid": "^8.3.1",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.5",
    "jest-sonar-reporter": "^2.0.0",
    "mockdate": "^3.0.5",
    "prettier": "^2.4.1",
    "randomstring": "^1.2.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jestSonar": {
    "reportPath": "reports"
  }
